{
  "canisters": {
    "eth_rpc": {
      "candid": "candid/eth_rpc.did",
      "type": "rust",
      "package": "eth_rpc"
    },
    "e2e_rust": {
      "dependencies": ["eth_rpc"],
      "candid": "e2e/rust/e2e_rust.did",
      "type": "rust",
      "package": "e2e"
    },
    "e2e_motoko": {
      "dependencies": ["eth_rpc"],
      "type": "motoko",
      "main": "e2e/motoko/Main.mo"
    }
  },
  "defaults": {
    "build": { "packtool": "npm run --silent sources" }
  },
  "version": 1
}
