{
  "canisters": {
    "evm_rpc": {
      "candid": "candid/evm_rpc.did",
      "type": "rust",
      "package": "evm_rpc",
      "declarations": {
        "bindings": ["did", "mo"],
        "output": "lib/motoko/declarations"
      }
    },
    "e2e_rust": {
      "dependencies": ["evm_rpc"],
      "candid": "e2e/rust/e2e_rust.did",
      "type": "rust",
      "package": "e2e"
    },
    "e2e_motoko": {
      "dependencies": ["evm_rpc"],
      "type": "motoko",
      "main": "e2e/motoko/Main.mo"
    }
  },
  "defaults": {
    "build": { "packtool": "npm run --silent sources" }
  },
  "version": 1
}
